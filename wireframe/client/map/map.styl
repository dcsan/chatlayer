cellHeight = 30px

.floorplan
  width 50%
  background-color #d2fffa
  
  .bub
    position absolute
    top -30px
    padding 5px
    background-color white
    border-radius 10px
    border 3px solid black
    font-size 20px
    color black
    z-index 200

  .stage
    border 1px solid black
    height cellHeight
    text-align center

  .tableBox
    width 100%

    .oneTable
      position relative
      display inline-block
      // border 1px solid #444
      background-color #222
      font-size 12px
      color white
      margin 1px
      width 11%
      height cellHeight
      text-align center
      cursor pointer

    .spacer
      display inline-block
      width 10px
