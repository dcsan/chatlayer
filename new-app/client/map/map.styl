cellHeight = 28px

.floorplan
  width 100%
  background-color #f7f7f7
  
  .bub
    position absolute
    top -30px
    padding 5px
    background-color white
    border-radius 4px
    border 1px solid black
    font-size 20px
    color black
    z-index 200

  .stage
    height cellHeight
    text-align center

  .tableBox
    width 100%

    .oneTable
      position relative
      display inline-block
      // border 1px solid #444
      background-color #333
      font-size 16px
      color white
      padding-top 8px
      margin 1px
      width 11%
      height cellHeight
      text-align center
      cursor pointer

    .spacer
      display inline-block
      width 20px
